{
  "name": "o3-shop/shop-demodata-installer",
  "description": "This tool is used to copy pictures from demo data repository to O3-Shop, during setup process.",
  "license": "GPL-3.0-only",
  "author": [
    {
      "name": "OXID eSales AG",
      "email": "info@oxid-esales.com",
      "homepage": "https://oxid-esales.com",
      "role": "Developer"
    },
    {
      "name": "O3-Shop",
      "email": "info@o3-shop.com",
      "homepage": "https://www.o3-shop.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "symfony/filesystem": "^4.4.17",
    "o3-shop/shop-facts": "^1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.14",
    "mikey179/vfsstream": "~1.6.8",
    "webmozart/path-util": ">=2.3"
  },
  "autoload": {
    "psr-4": {
      "OxidEsales\\DemoDataInstaller\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OxidEsales\\DemoDataInstaller\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "./bin/oe-eshop-demodata_install"
  ],
  "replace": {
    "oxid-esales/oxideshop-demodata-installer": "1.3.0"
  }
}
